{
  "name": "express-routes-file",
  "version": "1.0.5",
  "description": "express routing via rails / play-framework style 'routes' file",
  "author": {
    "name": "Dylan Nissley",
    "email": "dylan.nissley@gmail.com"
  },
  "license": "Unlicense",
  "repository": "github:dnissley/express-routes-file",
  "main": "lib/configureRoutes.js",
  "types": "lib/configureRoutes.d.ts",
  "files": [
    "./lib/"
  ],
  "scripts": {
    "lint": "standardx **/*.ts",
    "prepublishOnly": "tsc",
    "test": "npm run lint && jest"
  },
  "peerDependencies": {
    "express": "4.x"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.11",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "express": "^4.17.1",
    "jest": "^26.0.1",
    "standardx": "^5.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.5"
  },
  "standardx": {
    "ignore": [
      "./lib/**/*.js"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ]
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off",
      "@typescript-eslint/no-unused-vars": "error"
    }
  }
}
